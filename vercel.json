{
  "version": 2,
  "builds": [
    { "src": "api/cron-grab.ts", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/cron-grab", "dest": "/api/cron-grab.ts" }
  ],
  "crons": [
    { "path": "/api/cron-grab", "schedule": "0 0,12 * * *" }
  ]
} 